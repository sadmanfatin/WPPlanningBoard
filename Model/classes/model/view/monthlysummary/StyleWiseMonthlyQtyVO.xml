<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="StyleWiseMonthlyQtyVO"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.view.monthlysummary.StyleWiseMonthlyQtyVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[SELECT SQ.MONTH_ID,  
poc.buyer_id, 
 POC.BUYER_NAME,
 poc.style,  
 SUM( SQ.QUANTITY) WASH_QUANTITY,
 ROUND( SUM( SQ.QUANTITY) / (  (SELECT COUNT ( *)  FROM WP_HOLIDAY_CALENDAR HC WHERE HC.MONTH_ID  =  SQ.MONTH_ID AND  HC.WORKING_STATUS = 'Workingday') ) )   AVERAGE_PER_DAY,
   
 ROUND( ( SUM( SQ.QUANTITY)/  (SELECT SUM(QUANTITY )   FROM   WP_DAILY_STYLE_QUANTITY SQ2 WHERE SQ2.MONTH_ID = SQ.MONTH_ID )  )* 100) WASH_PERCENTAGE
FROM WP_DAILY_STYLE_QUANTITY SQ,
POC_SYSTEM_ID_VIEW  POC
WHERE SQ.SYSTEM_ID = POC.SYSTEM_ID
GROUP BY    SQ.MONTH_ID,  POC.BUYER_ID, POC.BUYER_NAME,poc.style]]>
  </SQLQuery>
