  <SQLQuery>
    <![CDATA[SELECT WpPlanningBoardEO.PLANNING_BOARD_ID, 
       WpPlanningBoardEO.MONTH_ID, 
       WpPlanningBoardEO.SYSTEM_ID, 
       WpPlanningBoardEO.REMARKS, 
       WpPlanningBoardEO.CREATED_BY, 
       WpPlanningBoardEO.CREATION_DATE, 
       WpPlanningBoardEO.LAST_UPDATED_BY, 
       WpPlanningBoardEO.LAST_UPDATED_DATE, 
       WpPlanningBoardEO.D1, 
       WpPlanningBoardEO.D2, 
       WpPlanningBoardEO.D3, 
       WpPlanningBoardEO.D4, 
       WpPlanningBoardEO.D5, 
       WpPlanningBoardEO.D6, 
       WpPlanningBoardEO.D7, 
       WpPlanningBoardEO.D8, 
       WpPlanningBoardEO.D9, 
       WpPlanningBoardEO.D10, 
       WpPlanningBoardEO.D11, 
       WpPlanningBoardEO.D12, 
       WpPlanningBoardEO.D13, 
       WpPlanningBoardEO.D14, 
       WpPlanningBoardEO.D15, 
       WpPlanningBoardEO.D16, 
       WpPlanningBoardEO.D17, 
       WpPlanningBoardEO.D18, 
       WpPlanningBoardEO.D19, 
       WpPlanningBoardEO.D20, 
       WpPlanningBoardEO.D21, 
       WpPlanningBoardEO.D22, 
       WpPlanningBoardEO.D23, 
       WpPlanningBoardEO.D24, 
       WpPlanningBoardEO.D25, 
       WpPlanningBoardEO.D26, 
       WpPlanningBoardEO.D27, 
       WpPlanningBoardEO.D28, 
       WpPlanningBoardEO.D29, 
       WpPlanningBoardEO.D30, 
       WpPlanningBoardEO.D31,
       WpPlanningBoardEO.ORG_ID,
       WpPlanningBoardEO.MONTHLY_TOTAL,
       PV.LC_UNIT,  
       APPS.ORG_ID_TO_ORG_NAME(LC_UNIT) LC_UNIT_NAME,       
       PV.BUYER_ID, 
       pv.BUYER_NAME ,
       PV.SEASON, 
       PV.STYLE ,   
       PV.COLOR, 
       PV.WASH_NAME ,
       SS.PRODUCTION_UNIT, 
       APPS.ORG_ID_TO_ORG_NAME(SS.PRODUCTION_UNIT) PRODUCTION_UNIT_NAME, 
        APPS.ORG_ID_TO_ORG_SHORT_NAME(SS.PRODUCTION_UNIT) PRODUCTION_UNIT_SHORT_NAME, 
       PV.ORDER_QTY ,
       SS.SAM_VERSION CURRENT_SAM_VERSION,       
      SS.AVAILABLE_SAM_VERSION   ,      
         SS.STYLE_SETUP_ID,
       ( SELECT SUM(QUANTITY) 
       FROM WP_DAILY_STYLE_QUANTITY SQ    
       WHERE SQ.SYSTEM_ID =   WPPLANNINGBOARDEO.SYSTEM_ID AND SQ.ORG_ID = WPPLANNINGBOARDEO.ORG_ID 
       AND SQ.MONTH_ID < WPPLANNINGBOARDEO.MONTH_ID    ) PREV_MONTHS_QTY, 
           
      PV.ORDER_QTY - (
      
      WPPLANNINGBOARDEO.MONTHLY_TOTAL + ( SELECT SUM(QUANTITY) 
       FROM WP_DAILY_STYLE_QUANTITY SQ    
       WHERE SQ.SYSTEM_ID =   WPPLANNINGBOARDEO.SYSTEM_ID AND SQ.ORG_ID = WPPLANNINGBOARDEO.ORG_ID 
       AND SQ.MONTH_ID < WPPLANNINGBOARDEO.MONTH_ID    )
      
      ) REMAINING_QTY
           
FROM WP_PLANNING_BOARD WpPlanningBoardEO, 
WP_STYLE_SETUP SS,
 POC_SYSTEM_ID_VIEW PV
   
 WHERE WpPlanningBoardEO.SYSTEM_ID = PV.SYSTEM_ID
 AND PV.SYSTEM_ID = SS.SYSTEM_ID
 AND WpPlanningBoardEO.ORG_ID = SS.ORG_ID]]>
  </SQLQuery>
