  <SQLQuery>
    <![CDATA[SELECT DISTINCT
POCL.SYSTEM_ID, 
POCH.LC_UNIT, 
  APPS.ORG_ID_TO_ORG_NAME(POCH.LC_UNIT) LC_UNIT_NAME, 
  
SS.PRODUCTION_UNIT, 
  APPS.ORG_ID_TO_ORG_NAME(SS.PRODUCTION_UNIT)   PRODUCTION_UNIT_NAME,
POCH.BUYER_ID, 
 APPS.XX_OM_BUYER_NAME_FOR_ID(POCH.BUYER_ID) BUYER_NAME ,
POCH.SEASON, 
POCH.STYLE ,   
POCL.COLOR, 
POCL.WASH_NAME , 
POCH.ORDER_QTY ,
ss.org_id,
       SS.SAM_VERSION CURRENT_SAM_VERSION,       
        (
            SELECT  nvl(  MAX(SV.SAM_VERSION),0) FROM  WP_STYLE_WISE_PROCESS_SAM_V SV  WHERE  SV.SYSTEM_ID = SS.SYSTEM_ID
            AND  SV.ORG_ID =  SS.ORG_ID
               
        )  AVAILABLE_SAM_VERSION     


FROM
   wp_style_setup ss,
 XX_OM_POC_H_T POCH,
XX_OM_POC_L_T POCL 
WHERE POCH.POC_ID = POCL.POC_ID
and ss.system_id = pocl.system_id

and not exists (select  pb.system_id from wp_planning_board pb where pb.system_id =  pocl.system_id
                 and  ss.org_id = (select org_id from wp_month_list ml where  ml.month_id = pb.month_id)
                    )
order by pocl.system_id desc]]>
  </SQLQuery>
