  Name="WashTypeWiseMonthlyQtyVO"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.view.monthlysummary.WashTypeWiseMonthlyQtyVO_LABEL"/>
    </SchemaBasedProperties>
