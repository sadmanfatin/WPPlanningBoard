  <SQLQuery>
    <![CDATA[SELECT   PB.MONTH_ID, ML.MONTH_NAME, ML.YEAR,  PB.ORG_ID,   SS.WASH_TYPE, SUM( PB.MONTHLY_TOTAL) TOTAL 
FROM WP_PLANNING_BOARD  PB,
WP_STYLE_SETUP SS,
WP_MONTH_LIST ML
WHERE PB.SYSTEM_ID  = SS.SYSTEM_ID
AND PB.ORG_ID = SS.ORG_ID
AND ML.MONTH_ID = PB.MONTH_ID
GROUP BY PB.MONTH_ID, ML.MONTH_NAME, ML.YEAR,  PB.ORG_ID,  SS.WASH_TYPE
order by TOTAL desc]]>
  </SQLQuery>
