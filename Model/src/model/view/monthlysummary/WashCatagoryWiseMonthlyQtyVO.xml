<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="WashCatagoryWiseMonthlyQtyVO"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.view.monthlysummary.WashCatagoryWiseMonthlyQtyVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[SELECT  distinct PB.MONTH_ID, 
ML.MONTH_NAME, 
ML.YEAR,  
PB.ORG_ID,    
APPS.WP_WASH_CATAGORY_F(SS.STYLE_SETUP_ID) WASH_CATAGORY,
sum (PB.MONTHLY_TOTAL) over(partition by PB.MONTH_ID, APPS.WP_WASH_CATAGORY_F(SS.STYLE_SETUP_ID))  TOTAL
FROM WP_PLANNING_BOARD  PB,
WP_STYLE_SETUP SS,
WP_MONTH_LIST ML
WHERE PB.SYSTEM_ID  = SS.SYSTEM_ID
AND PB.ORG_ID = SS.ORG_ID
AND ML.MONTH_ID = PB.MONTH_ID
--and ML.month_serial = :p 
--and APPS.WP_WASH_CATAGORY_F(SS.STYLE_SETUP_ID)   = :type
order by TOTAL desc]]>
  </SQLQuery>
