  <SQLQuery>
    <![CDATA[SELECT WpMonthListEO.MONTH_ID, 
       WpMonthListEO.MONTH_NAME, 
       WpMonthListEO.YEAR, 
       WpMonthListEO.ORG_ID, 
       WpMonthListEO.WEEKLY_HOLIDAY,
       WpMonthListEO.MONTH_NUMBER,
       WpMonthListEO.MONTH_SERIAL,
       extract (day from  last_day(to_date(WpMonthListEO.MONTH_NAME||'/'|| WpMonthListEO.YEAR , 'Month/yyyy' )) ) number_of_days,
       
       (select count ( *)  from WP_holiday_calendar hc where hc.month_id  =  WpMonthListEO.month_id and  hc.WORKING_STATUS = 'Workingday')  Workingday_counT,
       
       (select count ( *)  from WP_holiday_calendar hc where hc.month_id  =  WpMonthListEO.month_id and  hc.WORKING_STATUS = 'Holiday')  holiday_counT
       
FROM WP_MONTH_LIST WpMonthListEO]]>
  </SQLQuery>
